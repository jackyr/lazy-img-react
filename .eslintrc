{
  "env": {
      "browser": true
  },
  "extends": [
      "eslint:recommended", 
      "plugin:react/recommended"
  ],
  "globals": {
      "React": true
  },
  "parser": "babel-eslint",
  "rules": {
      "strict": 0,
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single"
      ],
      "react/no-find-dom-node": 0,
      "semi": [
          "error",
          "always"
      ],
      "no-console": [
          "warn"
      ]
  }
}
